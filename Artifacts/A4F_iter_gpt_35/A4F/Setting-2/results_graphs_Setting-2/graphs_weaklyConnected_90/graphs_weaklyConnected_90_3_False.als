sig Node { adj: set Node } pred undirected { adj = ~adj } pred oriented { no adj & ~adj } pred acyclic { all a: Node | a not in a.^adj } pred complete { all n: Node | n.adj = Node - n } pred noLoops { no iden & adj } pred weaklyConnected { all x: Node | all y: Node | some x.*(adj + ~adj).y } pred stronglyConnected { all n: Node | all m: Node | some n.*(adj + ~adj).m } pred transitive { adj.adj in adj } run {} for 5