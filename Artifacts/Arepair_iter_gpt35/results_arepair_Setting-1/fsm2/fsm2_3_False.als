one sig FSM { start: set State, stop: set State } sig State { transition: set State } // Part (a) fact OneStartAndStop { #FSM.start = 1 #FSM.stop = 1 } // Part (b) fact ValidStartAndStop { FSM.start != FSM.stop all s:State | FSM.start != s.transition no FSM.stop.transition } // Part (c) fact Reachability { State = FSM.start.*transition all s:(State - FSM.stop) | FSM.stop in s.*transition }